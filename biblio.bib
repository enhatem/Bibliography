@article{MELCHORAGUILAR2004175,
title = "On the Lyapunov's Indirect Method for Scalar Differential-Difference Equations",
journal = "IFAC Proceedings Volumes",
volume = "37",
number = "21",
pages = "175 - 180",
year = "2004",
note = "2nd IFAC Symposium on System Structure and Control, Oaxaca, Mexico, December 8-10, 2004",
issn = "1474-6670",
doi = "https://doi.org/10.1016/S1474-6670(17)30464-0",
url = "http://www.sciencedirect.com/science/article/pii/S1474667017304640",
author = "Daniel Melchor-Aguilar",
keywords = "nonlinear differential-difference equations, complete type Lyapunov-Krasovskii functionals",
abstract = "In this note a constructive proof of the well known Poincare-Lyapunov theorem for scalar differential-difference equations is given. The proof is based in a recent result on complete type Lyapunov-Krasovskii functionals to the robust stability analysis of time-delay systems. The proof is constructive and allow us to obtain an initial estimate of the region of attraction."
}
